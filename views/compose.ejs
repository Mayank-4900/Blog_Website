<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">

  <div class="form-group">
    <label for="newTitle">Title</label>
    <input class="form-control" type="text" name="blogTitle" autocomplete="off">
    <label for="newBlog">Post</label>
    <textarea class="form-control" name="blogContent" rows="8" cols="120"></textarea>
  </div>
  <button class="btn btn-primary" type="publish" name="button" style="background-color:blue">Publish</button>

</form>
<%- include("partials/footer") -%>
